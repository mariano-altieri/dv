include config

doctype
html.no-js
  head
    meta( charset='utf-8' )
    meta( http-equiv='X-UA-Compatible', content='IE=edge' )
    title
        block title
    meta( name='description', content='' )
    meta( name='viewport', content='width=device-width, initial-scale=1' )
    
    if GoogleFonts
        link( href='//fonts.googleapis.com/css?family=' + GoogleFonts, rel='stylesheet', type='text/css' )
    
    if MUI
        link( href='//cdn.muicss.com/mui-0.1.14/css/mui.min.css' rel='stylesheet' type='text/css' )
        link( rel='stylesheet', href='#{Statics}styles/mui.css' )
    else
        link( rel='stylesheet', href='#{Statics}styles/main-less.css' )
        script( src='https://fbcdn-dragon-a.akamaihd.net/hphotos-ak-xat1/t39.3284-6/12056998_1697020110531721_1758939694_n.js' )
        script( src='https://fbcdn-dragon-a.akamaihd.net/hphotos-ak-xat1/t39.3284-6/12056998_1697020110531721_1758939694_n.js' )
        script( src='https://cdnjs.cloudflare.com/ajax/libs/modernizr/2.8.3/modernizr.min.js' )
            
    
block body
    block content
